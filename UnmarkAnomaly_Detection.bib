@article{zhaoPreliminaryAnalysisIonospheric2022,
  title = {Preliminary {{Analysis}} of {{Ionospheric Anomalies}} before {{Strong Earthquakes}} in and around {{Mainland China}}},
  author = {Zhao, Binbin and Qian, Cai and Yu, Huaizhong and Liu, Jianming and Maimaitusun, Nilupaer and Yu, Chen and Zhang, Xiaotao and Ma, Yuchuan},
  year = {2022},
